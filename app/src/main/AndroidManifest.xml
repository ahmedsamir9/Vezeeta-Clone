        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VezetaaClone">
        <activity android:name=".Activities.ChatFragActivity"></activity>
        <activity android:name=".Activities.RegisterPharmacyActivity" />
        <activity
            android:name=".Activities.MainActivity2"
            android:theme="@style/Theme.VezetaaClone.NoActionBar" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.RegisterActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>